# General Settings
restapi.endpoint=http://localhost:8080/notifications # REST API endpoint
restapi.username=user # Username for HTTP authentication
restapi.password=password # Password for HTTP authentication
restapi.benchmark.request.type=ALL # REST API method, choose from: POST, GET, PUT, DELETE, or use ALL to test every method
restapi.benchmark.request.total=10000 # Total number of requests to make per run
restapi.benchmark.request.concurrent=1000 # Maximum number of concurrent connections
restapi.benchmark.timelimit=60 # Max amount of time in seconds for the test to run
restapi.benchmark.runs=1 # Number of times to run the test
restapi.benchmark.notification.id.output.file=ids.txt # Output file for object ids that are stored to the REST API